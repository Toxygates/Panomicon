<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0" 
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:noNamespaceSchemaLocation=
                   "http://ant.apache.org/ivy/schemas/ivy.xsd">
  <info organisation="level-five" module="t" />
        
  <dependencies>

  <!-- for GWT upload -->
  <!-- This fails to resolve some of its dependencies as of 15/1/27.
  In addition, pulls in all of GWT -->
  <!-- r 1.0.3 is compatible with GWT 2.7 -->
  <!-- <dependency org="com.googlecode.gwtupload" name="gwtupload" rev="1.0.3"/> -->
    
  <dependency org="commons-fileupload" name="commons-fileupload" rev="latest.integration"/>
  <dependency org="commons-io" name="commons-io" rev="latest.integration"/>   
  <dependency org="javax.mail" name="mail" rev="latest.integration"/>
  
  <!-- intermine is not published on maven central, but the POM is at the URL below. -->
  <!-- https://github.com/alexkalderimis/intermine-ws-client.java/blob/master/pom.xml -->  
  <!-- <dependency org="org.intermine.webservice.client" name="intermine-webservice-client" rev="latest.integration"/> -->
  
  <!-- for intermine -->
  <!--
  <dependency org="stax" name="stax-api" rev="1.0.1"/>
  <dependency org="stax" name="stax" rev="1.2.0"/>
    -->
  
  <!-- for intermine.  -->
  <!-- Note that the latest version of json is not compatible with intermine-client-2.0 
    (causes parse errors). 20090211 is known to work. -->
  <dependency org="org.json" name="json" rev="20090211"/>
  <dependency org="commons-lang" name="commons-lang" rev="latest.integration"/>
  <!-- intermine-client-2.0 needs the old commons-httpclient module
     (org.apache.httpcomponents will not work) -->
  <!-- <dependency org="org.apache.httpcomponents" name="fluent-hc" rev="latest.integration"/> -->
  <dependency org="commons-httpclient" name="commons-httpclient" rev="latest.integration"/> 
  
  <!-- for intermine. -->
  <dependency org="log4j" name="log4j" rev="latest.integration" />

  <dependency org="org.antlr" name="antlr" rev="latest.integration" />

  <!-- for R -->
  <!-- r 1.8.1 is known to work -->  
  <dependency org="org.rosuda.REngine" name="Rserve" rev="latest.integration"/>
  
  </dependencies>
</ivy-module>
